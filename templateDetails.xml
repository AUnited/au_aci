<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE extension PUBLIC "-//Joomla! 3.0//DTD template 1.0//EN" "http://help.aunited.ru/jdtd/3.0/template-install.dtd">
<extension version="3.5" type="template" client="site" method="upgrade">
	<name>au_aci</name>
	<version>0.1.0</version>
	<creationDate>August 2016</creationDate>
	<author>Amvis United Company Limited</author>
	<authorEmail>info@aunited.ru</authorEmail>
	<authorUrl>http://aunited.ru</authorUrl>
    <copyright>Copyright (C) Aunited Company Limited</copyright>
    <license>http://www.gnu.org/licenses/gpl.html GNU/GPL</license>
	<description>Based on Master theme of the YOOtheme club. It is based on YOOtheme's Warp theme framework.</description>
	<files>
		<filename>checksums</filename>
		<filename>apple_touch_icon.png</filename>
		<filename>CHANGELOG.md</filename>
		<filename>component.php</filename>
		<filename>config.php</filename>
		<filename>config.xml</filename>
		<filename>config.default.json</filename>
		<filename>error.php</filename>
		<filename>favicon.ico</filename>
		<filename>index.php</filename>
		<filename>offline.php</filename>
		<filename>raw.php</filename>
		<filename>warp.php</filename>
		<filename>template_preview.png</filename>
		<filename>template_thumbnail.png</filename>
		<filename>templateDetails.xml</filename>
		<folder>css</folder>
		<folder>html</folder>
		<folder>js</folder>
		<folder>layouts</folder>
		<folder>less</folder>
		<folder>warp</folder>
	</files>
    <positions>
        <position>search</position>
        <position>logo</position>
        <position>logo-small</position>
        <position>menu</position>
        <position>toolbar-l</position>
        <position>toolbar-r</position>
        <position>headerbar</position>
        <position>breadcrumbs</position>
        <position>top-a</position>
        <position>top-b</position>
        <position>bottom-a</position>
        <position>bottom-b</position>
        <position>main-top</position>
        <position>main-bottom</position>
        <position>sidebar-a</position>
        <position>sidebar-b</position>
        <position>footer</position>
        <position>offcanvas</position>
        <position>debug</position>
    </positions>
	<config>
		<fields name="params" addfieldpath="/templates/au_aci/warp/systems/joomla/config/elements">
			<fieldset name="options">
		    	<field name="config" type="config" label="Config" description="" default="" />
			</fieldset>
		</fields>
	</config>
</extension>
